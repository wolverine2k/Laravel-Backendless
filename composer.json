{
    "name": "naresh-mehta/backendless",
    "description": "BackendlessServiceProvider Is A Backendless SDK Bridge For Laravel 5",
    "keywords": ["laravel", "framework", "backendless", "backendless-sdk", "backendless sdk", "BackendlessSDK", "backendless bridge", "backendless sdk bridge", "bridge", "Backendless", "Laravel Backendless", "Laravel-Backendless", "Naresh Mehta", "NareshMehta"],
    "license": "MIT",
    "authors": [
        {
            "name": "Naresh Mehta",
            "email": "naresh.mehta@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/contracts": "~5.1",
        "illuminate/support": "~5.1"
    },
   "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "NareshMehta\\Backendless\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NareshMehta\\Tests\\Backendless\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
